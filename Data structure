#include<stdio.h>
#include<stdlib.h>
Typedef struc
{
char name[30];
int age,phn_no,salary;
}
Employee;
int main()
{
int n=20;
Employee employees[20];
printf("Enter %d employee details\n\n",n);
for(int i=0;i<n;i++)
{
printf("Employee %d:\n",i+1);
printf("Name:");
scanf("%s",employees[i].name);
printf("Age:");
scanf("%d",&employees[i].age);
printf("phn_no:");
scanf("%d",&employees[i].phn_no);
printf("salary:");
scanf("%d",&employees[i].salary);
printf("\n");
}
print("----------------------------------ALL EMPLOYEE DETAILS-----------------------------------------\n");
for(i=0;i<n;i++)
{
printf("Name\t:");
scanf("%s\n",&employees[i].name);
printf("age\t:");
scanf("%d\n",employees[i].age);
printf("phn_no.\t:");
scanf("%d\n",employees[i].phn_no);
printf("salary\t:");
scanf("%d\n",employees[i].salary);
printf("\n");
}
return 0;
}
